using System;
class age
{
    private int Age;
    public void showage()
    {
        Console.WriteLine("enter a age");
        Age =Convert.ToInt32(Console.ReadLine());   
        Console.WriteLine("age is " +Age);
    }
}
class program
{
    static void Main()
    {
        age a=new age();
        a.showage();
    }
}
